{
    "name": "react4xp",
    "version": "1.0.0",
    "description": "React starter for Enonic XP Platform",
    "scripts": {
        "build": "npm i; ./gradlew build",
        "deploy": "./gradlew deploy",
        "build:xp": "./gradlew babelXP",
        "watch:xp": "./gradlew deploy -t",
        "rm": "node_modules/rimraf/bin.js build",
        "build:r4xp": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.js --color --progress",
        "watch:r4xp": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.js --color --progress --watch",
        "build:r4xp-back": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.backCore.js --color --progress",
        "watch:r4xp-back": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.backCore.js --color --progress --watch",
        "build:r4xp-front": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.frontCore.js --color --progress",
        "watch:r4xp-front": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.frontCore.js --color --progress --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enonic/starter-react.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/enonic/starter-react/issues"
    },
    "homepage": "https://github.com/enonic/starter-react#readme",
    "dependencies": {
        "immutable": "^3.8.2",
        "jvm-npm": "^0.1.1",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "4.4.0-alpha.1",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.0",
        "redux-immutable": "^4.0.0",
        "warning": "^4.0.1",
        "workbox-webpack-plugin": "^2.1.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "clean-webpack-plugin": "^1.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^0.28.11",
        "html-webpack-plugin": "^3.2.0",
        "less-loader": "^4.1.0",
        "rimraf": "^2.6.3",
        "style-loader": "^0.21.0",
        "webpack": "^4.28.2",
        "webpack-cli": "^3.1.2"
    },
    "babel": {
        "presets": [
            "es2015",
            "react",
            "stage-0"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    }
}
