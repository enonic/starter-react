{
    "name": "react4xp",
    "version": "0.0.1",
    "description": "React starter for Enonic XP Platform",
    "scripts": {
        "rm": "node_modules/rimraf/bin.js build",
        "rebuild": "npm run rm; npm i; ./gradlew build",
        "deploy": "./gradlew deploy --info",
        "watch": "./gradlew deploy --info -t",
        "babel:xp": "./gradlew babelXP",
        "rebuild:r4xp": "npm run rm; node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.js --progress",
        "watch:r4xp": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.js --progress --watch",
        "rebuild:r4xp-ext": "npm run rm; node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.externals.js --progress",
        "watch:r4xp-ext": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.externals.js --progress --watch",
        "rebuild:r4xp-core": "npm run rm; node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.frontCore.js --progress",
        "watch:r4xp-core": "node_modules/webpack/bin/webpack.js --config webpack.config.react4xp.frontCore.js --progress --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enonic/starter-react.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/enonic/starter-react/issues"
    },
    "homepage": "https://github.com/enonic/starter-react#readme",
    "dependencies": {
        "duckfactory": "^1.3.4",
        "immutable": "^3.8.2",
        "jvm-npm": "^0.1.1",
        "react": "^16.4.1",
        "react-dom": "^16.4.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "4.4.0-alpha.1",
        "react-router-redux": "^4.0.8",
        "redux": "^4.0.0",
        "redux-immutable": "^4.0.0",
        "warning": "^4.0.1",
        "workbox-webpack-plugin": "^2.1.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "chunks-2-json-webpack-plugin": "^1.0.0",
        "css-loader": "^0.28.11",
        "es6-set-and-map": "^1.0.5",
        "less-loader": "^4.1.0",
        "react4xp-build-entriesandchunks": "0.0.1",
        "react4xp-buildconstants": "^3.3.1",
        "rimraf": "^2.6.3",
        "style-loader": "^0.21.0",
        "webpack": "^4.28.2",
        "webpack-cli": "^3.1.2"
    },
    "babel": {
        "presets": [
            "es2015",
            "react",
            "stage-0"
        ],
        "plugins": [
            "transform-object-rest-spread"
        ]
    }
}
